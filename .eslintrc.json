{
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "no-console": "off",
        "indent": [
            "error", 4, {
                "ignoreComments": true
            }
        ],
        "comma-dangle": ["error", "never"]
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "jasmine": true
    }
}